    "babel-loader": "~7.1.4",
    "babel-preset-es2015": "~6.24.1",
    "babel-preset-react": "~6.24.1",
    "cross-env": "~5.0.1",
    "electron": "~9.1.0",
    "electron-builder": "~22.8.1",
    "enzyme": "~3.3.0",
    "enzyme-adapter-react-16": "~1.1.1",
    "enzyme-to-json": "~3.3.1",
    "fetch-mock": "~5.13.1",
    "foreman": "~3.0.1",
    "jest": "~20.0.4",
    "jest-cli": "~20.0.4",
    "jest-junit": "~3.6.0",
    "jest-localstorage-mock": "~2.2.0",
    "jest-styled-components": "~5.0.0",
    "jsdom": "~11.6.2",
    "moxios": "~0.4.0",
    "react-addons-test-utils": "~15.6.0",
    "react-scripts": "~1.1.5",
    "react-test-renderer": "~16.2.0",
    "sinon": "~4.4.3",
    "web-audio-mock": "~0.1.9"
  },
  "scripts": {
    "start": "nf start -p 3000",
    "build": "cross-env NODE_PATH=src react-scripts build",
    "test": "cross-env NODE_PATH=src react-scripts test",
    "test:ci": "cross-env NODE_PATH=src react-scripts test --env=jsdom --testResultsProcessor ./node_modules/jest-junit --coverage --collectCoverageFrom=src/js/**/*.js --coverageDirectory=src/__test__/coverage",
    "test_unit": "cross-env NODE_PATH=src react-scripts test --env=jsdom --testPathPattern=unit_tests --coverage",
    "test_integration": "cross-env NODE_PATH=src react-scripts test --env=jsdom --testPathPattern=integration_tests --coverage",
    "eject": "react-scripts eject",
    "coverage": "npm run test:ci -- --coverage --ci",
    "electron": "electron .",
    "electron-start": "node src/start-react",
    "react-start": "react-scripts start",
    "pack": "build --dir",
    "dist": "npm run build && electron-builder --publish never -wlm",
    "dist-win": "npm run build && electron-builder --publish never --win",
    "dist-linux": "npm run build && electron-builder --publish never --linux",
    "dist-mac": "npm run build && electron-builder --publish never --mac",
    "postinstall": "install-app-deps"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "src/js/**/*.{js,jsx}",
      "!**/node_modules/**",
      "!src/localState.js",
      "!src/reduxConstants.js",
      "!src/store.js"
    ],
    "coverageReporters": [
      "lcov",
      "text"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 30,
        "functions": 30,
        "lines": 30,
        "statements": -10000
      }
    }
  },
  "jest-junit": {
    "suitName": "jest tests",
    "output": "src/__test__/junit.xml",
    "classNameTemplate": "{classname} - {title}",
    "titleTemplate": "{classname} - {title}",
    "ancestorSeparator": " > ",
    "usePathForSuitName": "true"
  },
  "proxy": "http://bttExchangerAdmin:8080",
  "build": {
    "appId": "com.electron.btt.exchanger.admin",
    "linux": {},
    "win": {
      "icon": "./public/logo.ico"
    },
    "mac": {
      "category": "public.app-category.education",
      "icon": "./public/logo.icns",
      "target": "zip"
    },
    "dmg": {
      "icon": "./public/logo.icns"
    },
    "directories": {
      "buildResources": "public"
    },
    "extends": null,
    "asar": true
  }
}
